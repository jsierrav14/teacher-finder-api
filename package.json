{
  "name": "teacherfinder-api",
  "version": "1.0.0",
  "description": "This is the api rest for teacherFinder app",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "node ./build/index.js",
    "dev": "env-cmd -f ./config/dev.env nodemon --exec babel-node src/index.js"
  },
  "keywords": [
    "api",
    "rest",
    "tech",
    "teacher",
    "finder",
    "students"
  ],
  "author": "Jose Sierra",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.19",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-syntax-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "babel-polyfill": "^6.26.0",
    "env-cmd": "^10.1.0",
    "install": "^0.13.0",
    "nodemon": "^2.0.2"
  }
}
